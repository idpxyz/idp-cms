# =============================================================================
# 开发环境特定配置
# =============================================================================
# 仅在开发环境中使用的配置

# =============================================================================
# Django 开发配置
# =============================================================================
DJANGO_DEBUG=1
DJANGO_SETTINGS_MODULE=config.settings.dev

# =============================================================================
# 日志和调试配置
# =============================================================================
LOG_LEVEL=DEBUG
NODE_ENV=development

# =============================================================================
# 开发工具配置
# =============================================================================
ENABLE_DEBUG_TOOLBAR=true
ENABLE_SILK_PROFILER=false
ENABLE_DJANGO_EXTENSIONS=true
